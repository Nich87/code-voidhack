{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "https://code-voidhack.vercel.app"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "Content-Security-Policy",
					"value": "style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' https://voidhacker.com/ https://code-voidhack.vercel.app"
				},
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=63072000; includeSubDomains; preload"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin-when-cross-origin"
				}
			]
		}
	],
	"trailingSlash": false
}
